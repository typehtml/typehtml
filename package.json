{
  "name": "typehtml",
  "version": "0.0.0",
  "description": "Simple HTML with the safety of TypeScript",
  "main": "lib/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "karma:firefox": "karma start lib/tests/karma/karma.config.js --browsers=Firefox",
    "karma:chrome": "karma start lib/tests/karma/karma.config.js --browsers=Chrome",
    "karma": "npm run karma:firefox && npm run karma:chrome",
    "testem": "testem",
    "build": "tsc -p .",
    "unittest": "mocha ./lib/tests/mocha/**/*.js",
    "test": "npm run build && npm run unittest && npm run karma && npm run testem -- ci",
    "start": "npm run build -- -w & npm run unittest -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typehtml/typehtml.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/typehtml/typehtml/issues"
  },
  "homepage": "https://github.com/typehtml/typehtml#readme",
  "devDependencies": {
    "@types/mocha": "^2.2.38",
    "@types/node": "^7.0.4",
    "browserify": "^13.0.0",
    "karma": "^1.4.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-failed-reporter": "0.0.3",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "2.0.1",
    "mocha": "^3.2.0",
    "testem": "^1.0.2",
    "ts-loader": "^2.0.0",
    "typescript": "2.1.5",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0"
  }
}
